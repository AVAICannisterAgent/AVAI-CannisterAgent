{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production"
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
